{
  "title": "Avito Realty input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "citySlug": {
      "title": "Город (slug)",
      "type": "string",
      "description": "Например: moskva, sanktpeterburg, ekaterinburg. Пусто — вся Россия.",
      "editor": "textfield",
      "default": "moskva"
    },
    "category": {
      "title": "Категория",
      "type": "string",
      "enum": [
        "kvartiry",
        "doma_dachi_kottedzhi",
        "komnaty",
        "zemelnye_uchastki",
        "garazhi_i_mashinomesta",
        "kommercheskaya_nedvizhimost"
      ],
      "editor": "select",
      "default": "kvartiry"
    },
    "minPrice": {
      "title": "Мин. цена",
      "type": "integer",
      "editor": "number",
      "default": 0
    },
    "maxPrice": {
      "title": "Макс. цена",
      "type": "integer",
      "editor": "number",
      "default": 0
    },
    "rooms": {
      "title": "Комнат",
      "type": "array",
      "items": {
        "type": "integer",
        "enum": [0, 1, 2, 3, 4, 5]
      },
      "editor": "multiselect",
      "default": []
    },
    "withImages": {
      "title": "Только с фото",
      "type": "boolean",
      "editor": "checkbox",
      "default": true
    },
    "onlyOwner": {
      "title": "Только от собственника",
      "type": "boolean",
      "editor": "checkbox",
      "default": false
    },
    "pagesLimit": {
      "title": "Лимит страниц",
      "type": "integer",
      "editor": "number",
      "default": 5
    },
    "maxItems": {
      "title": "Макс. объявлений",
      "type": "integer",
      "editor": "number",
      "default": 0
    },
    "proxy": {
      "title": "Apify Proxy groups",
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Рекомендуется использовать RESIDENTIAL или SHADER+RESIDENTIAL",
      "editor": "multiselect",
      "default": ["RESIDENTIAL"]
    }
  },
  "required": []
}
